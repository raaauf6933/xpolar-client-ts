{
  "name": "react-starter-kit-ts",
  "version": "1.0.0",
  "description": "",
  "main": "index.jsx",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --env NODE_ENV=development --open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tsc --noEmit && eslint .",
    "check-types": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "prettier": "prettier --ignore-path .gitignore \"src/**/*.+(json|js|ts|tsx)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier -c",
    "build-types": "graphql-codegen --config codegen.ts",
    "build-types-v1": "apollo client:codegen --target=typescript types --globalTypesFile=src/types/globalTypes.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "resolutions": {
    "graphql": "15.5.3"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-typescript": "^7.18.6",
    "autoprefixer": "^10.4.13",
    "classnames": "^2.3.2",
    "dotenv-webpack": "^8.0.1",
    "formik": "^2.2.9",
    "graphql": "15.5.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.0",
    "react-svg": "^15.1.16",
    "ts-loader": "^9.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@graphql-codegen/cli": "2.16.2",
    "@graphql-codegen/client-preset": "1.2.4",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/lodash": "^4.14.191",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "apollo": "^2.34.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "esbuild-loader": "^2.20.0",
    "eslint": "^8.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-tailwindcss": "^3.7.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.19",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  }
